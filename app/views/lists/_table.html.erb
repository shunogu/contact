<table class="striped">
  <thead>
    <tr>
      <th>Company name</th>
      <th>Person</th>
      <th>Memo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lists.each do |list| %>
      <tr>
        <td><%= list.company %></td>
        <td><%= list.person %></td>
        <td><%= list.memo %></td>
        <td><%= link_to 'Show', list, class: "btn-small" %></td>
        <td><%= link_to 'Edit', edit_list_path(list), class: "btn-small edit" %></td>
        <td><%= link_to 'Destroy', list, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn-small destroy" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%#= paginate @lists %>
